<?xml version="1.0" encoding="UTF-8"?>
<keymap name="default">
    <action id="cut up to line end." plugin="Action_CutUpToLineEnd">
        <keyboard-shortcut component="JTextArea" condition="0" keystroke="ctrl K"/>
        <keyboard-shortcut component="TWTextArea" condition="0" keystroke="ctrl K"/>
        <keyboard-shortcut component="WTextArea" condition="1" keystroke="meta K"/>
    </action>
    <action id="paste" plugin="Action_Paste">
        <keyboard-shortcut component="JTextArea" condition="0" keystroke="ctrl Y"/>
        <keyboard-shortcut component="TWTextArea" condition="1" keystroke="meta K"/>
        <keyboard-shortcut component="TWTextArea" condition="2" keystroke="meta K"/>
        <keyboard-shortcut component="TWTextArea" condition="0" keystroke="meta Y"/>
    </action>
    <action id="retweet" plugin="Action_Retweet"/>
</keymap>
