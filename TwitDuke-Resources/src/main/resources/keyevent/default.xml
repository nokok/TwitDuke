<?xml version="1.0" encoding="UTF-8"?>
<keymap name="default">
    <action id="cut up to line end." plugin="net.nokok.twitduke.components.actions.Action_CutUpToLineEnd">
        <keyboard-shortcut component="net.nokok.twitduke.core.view.TweetTextArea" condition="0" keystroke="ctrl K"/>
        <keyboard-shortcut component="net.nokok.twitduke.core.view.TweetTextArea" condition="0" keystroke="meta K"/>
    </action>
    <action id="paste" plugin="net.nokok.twitduke.components.actions.Action_Paste">
        <keyboard-shortcut component="net.nokok.twitduke.core.view.TweetTextArea" condition="0" keystroke="ctrl Y"/>
        <keyboard-shortcut component="net.nokok.twitduke.core.view.TweetTextArea" condition="0" keystroke="meta Y"/>
    </action>
    <action id="hey!" plugin="net.nokok.twitduke.components.actions.Action_Hey">
        <keyboard-shortcut component="net.nokok.twitduke.components.basics.TWPanel" condition="1"
                           keystroke="ctrl O"/>
    </action>
    <action id="retweet" plugin="Action_Retweet"/>
</keymap>
